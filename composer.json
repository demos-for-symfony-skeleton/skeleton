{
    "name": "symfony-flex-demo/console",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Console Flex project",
    "require": {
        "symfony/console": "^3.4 || ^4.0",
        "symfony/flex": "^1.0",
        "symfony/yaml": "^3.4 || ^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4 || ^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "bin": ["bin/console"],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
